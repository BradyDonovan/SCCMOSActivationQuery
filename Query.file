select SMS_R_SYSTEM.ResourceID,SMS_R_SYSTEM.ResourceType,SMS_R_SYSTEM.Name,SMS_R_SYSTEM.SMSUniqueIdentifier,SMS_R_SYSTEM.ResourceDomainORWorkgroup,SMS_R_SYSTEM.Client from SMS_R_System inner join SMS_G_System_SOFTWARE_LICENSING_PRODUCT on SMS_G_System_SOFTWARE_LICENSING_PRODUCT.ResourceID = SMS_R_System.ResourceId where SMS_G_System_SOFTWARE_LICENSING_PRODUCT.ApplicationID = "55c92734-d682-4d71-983e-d6ec3f16059f" and (SMS_G_System_SOFTWARE_LICENSING_PRODUCT.LicenseStatus != 0 and SMS_G_System_SOFTWARE_LICENSING_PRODUCT.LicenseStatus != 1)
